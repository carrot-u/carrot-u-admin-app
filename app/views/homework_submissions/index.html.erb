<p id="notice"><%= notice %></p>

<h1>Homework Submissions</h1>

<table>
  <thead>
    <tr>
      <th>Course session user</th>
      <th>Homework</th>
      <th>Pull request</th>
      <th>Is public</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @homework_submissions.each do |homework_submission| %>
      <tr>
        <td><%= homework_submission.course_session_participant_id %></td>
        <td><%= homework_submission.homework_id %></td>
        <td><%= homework_submission.pull_request %></td>
        <td><%= homework_submission.is_public %></td>
        <td><%= link_to 'Show', homework_submission %></td>
        <td><%= link_to 'Edit', edit_homework_submission_path(homework_submission) %></td>
        <td><%= link_to 'Destroy', homework_submission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Homework Submission', new_homework_submission_path %>
